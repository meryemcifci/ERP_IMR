            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.COMCODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UNITTEXT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ISMAINUNIT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MAINUNITCODE)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UNITCODE">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UNITCODE">Details</a> |
