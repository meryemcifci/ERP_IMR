@model IEnumerable<ERP_IMR.Models.BSMIMRGEN001>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.COMTEXT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ADDRESS1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ADDRESS2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CITYCODE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.COUNTRYCODE)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.COMTEXT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ADDRESS1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ADDRESS2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CITYCODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COUNTRYCODE)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.COMCODE">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.COMCODE">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.COMCODE">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
